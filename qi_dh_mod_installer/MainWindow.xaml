<Window x:Class="qi_dh_mod_installer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:qi_dh_mod_installer"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Initialized="Window_Initialized">
    <Grid>
        <Button x:Name="B_install" Content="安装&#xD;&#xA;install" HorizontalAlignment="Center" Height="60" Margin="0,334,0,0" VerticalAlignment="Top" Width="140" Click="B_install_Click"/>
        <TextBox x:Name="tb_t" HorizontalAlignment="Left" Margin="200,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="500" Height="190" Visibility="Hidden" IsReadOnly="True" IsUndoEnabled="False"/>
        <Button x:Name="b_log" Content="LOG" HorizontalAlignment="Left" Margin="748,379,0,0" VerticalAlignment="Top" Click="b_log_Click"/>
        <TextBlock x:Name="tb_version" HorizontalAlignment="Left" Margin="30,33,0,0" TextWrapping="Wrap" Text="MOD V1.0.3" VerticalAlignment="Top"/>
        <Label Content="Label" HorizontalAlignment="Left" Margin="280,129,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
